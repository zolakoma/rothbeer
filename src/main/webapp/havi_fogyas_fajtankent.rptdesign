            <xml-property name="queryText"><![CDATA[select concat(YEAR(o.outStoreDate), '.', MONTH(o.outStoreDate), '.') as month, 
 sum(oi.quantity * pack.VOLUME) AS osszes,
 min(SUBSTRING_INDEX(p.NAME, ' ', 1)) as fajta,
 sum(case when pack.volume < 1 THEN oi.quantity * pack.VOLUME ELSE 0 END) AS palackos,
 sum(case when pack.volume > 1 THEN oi.quantity * pack.VOLUME ELSE 0 END) AS hordos
from `order` o 
	join orderitem oi on oi.order_ = o.id
    join product p on oi.product = p.id
    join packing pack on pack.id = p.packing
group by  YEAR(o.outStoreDate), MONTH(o.outStoreDate),SUBSTRING_INDEX(p.NAME, ' ', 1)]]></xml-property>
